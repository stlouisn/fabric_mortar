{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name} [C]",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "${url_homepage}",
    "sources": "${git_sources}",
    "issues": "${git_issues}"
  },
  "license": "${mod_license}",
  "icon": "/logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.mortar.Mortar"
    ]
  },
  "mixins": [
    "mortar.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "${minecraft_version}"
  },
  "recommends": {
    "modmenu": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.website": "${url_homepage}"
      }
    }
  }
}